{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "archtechx/enums": "^0.2.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.2",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "spatie/laravel-json-api-paginate": "^1.11",
        "spatie/laravel-query-builder": "^5.0",
        "tymon/jwt-auth": "*",
        "spatie/laravel-slack-alerts": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "brianium/paratest": "^6.4",
        "darkaonline/l5-swagger": "^8.3",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.6",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpro/grumphp": "^1.8",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "vimeo/psalm": "^4.22"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "check-build": [
            "psalm --no-cache",
            "php-cs-fixer fix --dry-run --verbose --using-cache=no --config=.php_cs.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "hooks": {
            "pre-commit": [
                "php-cs-fixer fix --verbose --config=.php_cs.php",
                "psalm"
            ]
        },
        "grumphp": {
            "config-default-path": "./grumphp.yml"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
